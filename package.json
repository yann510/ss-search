    "ts-node": "10.9.1",
    "tsup": "8.3.5",
    "typescript": "5.5.4",
    "vite": "5.4.14",
    "vitest": "2.1.9"
  },
  "nxrelease": {
    "repositoryUrl": "https://github.com/yann510/ss-search",
    "tagFormat": "v${version}",
    "buildTarget": "ss-search:build",
    "outputPath": "dist/ss-search",
    "gitAssets": [
      "CHANGELOG.md",
      "package.json",
      "package-lock.json",
      "ss-search/package.json",
      "ss-search/package-lock.json",
