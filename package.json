    "react-highlight-words": "0.20.0",
    "react-router-dom": "6.15.0",
    "recharts": "2.8.0",
    "tslib": "2.6.2",
    "tss-react": "4.9.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.16.4",
    "@nx/esbuild": "19.8.8",
    "@nx/eslint": "19.8.8",
    "@nx/eslint-plugin": "19.8.8",
    "@nx/jest": "19.8.8",
    "@nx/js": "19.8.8",
    "@nx/node": "19.8.8",
    "@nx/react": "19.8.8",
    "@nx/rollup": "19.8.8",
    "@nx/rspack": "19.8.0",
    "@nx/vite": "19.8.8",
