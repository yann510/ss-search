    "@rspack/plugin-minify": "0.7.5",
    "@rspack/plugin-react-refresh": "1.0.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "0.3.14",
    "@swc/core": "1.5.7",
    "@testing-library/react": "15.0.6",
    "@types/benchmark": "2.1.2",
    "@types/jest": "29.5.14",
    "@types/js-search": "1.4.0",
    "@types/lodash": "4.14.198",
