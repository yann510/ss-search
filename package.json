    "lunr": "2.3.9",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-highlight-words": "0.21.0",
    "react-router-dom": "7.8.2",
    "recharts": "3.1.2",
    "tslib": "2.8.1",
    "tss-react": "4.9.19"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@nx/esbuild": "21.4.1",
    "@nx/eslint": "21.4.1",
    "@nx/eslint-plugin": "21.4.1",
    "@nx/jest": "21.4.1",
    "@nx/js": "21.4.1",
    "@nx/node": "21.4.1",
    "@nx/react": "21.4.1",
    "@nx/rollup": "21.4.1",
    "@nx/rspack": "21.4.1",
